# platform :ios, “7.0”
target ‘BaseProject’ do

pod 'AFNetworking'
pod 'SDWebImage'
pod 'MBProgressHUD'
pod 'Masonry'
pod 'MJExtension'
pod 'IQKeyboardManager'
pod 'ZYBannerView'
pod 'PopupDialog', '~> 0.5'

use_frameworks!

end

post_install do |installer|
    installer.pods_project.targets.each do |target|
        target.build_configurations.each do |config|
            config.build_settings['SWIFT_VERSION'] = '3.0'
        end
    end
end
